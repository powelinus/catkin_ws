<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
	<rosparam file="$(find mbsrobot)/param/navigation/$(env MBSBASE)/costmap_common_params.yaml" command="load" ns="global_costmap" />
	<rosparam file="$(find mbsrobot)/param/navigation/$(env MBSBASE)/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find mbsrobot)/param/navigation/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find mbsrobot)/param/navigation/global_costmap_params.yaml" command="load" />
	<rosparam file="$(find mbsrobot)/param/navigation/$(env MBSBASE)/base_local_planner_params.yaml" command="load" />
    <rosparam file="$(find mbsrobot)/param/navigation/move_base_params.yaml" command="load" />
  </node>
</launch>
